{
  "name": "reactive-di",
  "version": "3.2.0",
  "description": "Reactive dependency injection",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "config": {
    "dest": "./out"
  },
  "scripts": {
    "prepublish": "npm run build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepush": "npm test",
    "update": "ncu -ua && npm install",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --ignore=__tests__ --source-maps --out-dir dist",
    "build.dep": "npm-helpers/build-deps.sh",
    "build.dev": "npm run build -- --watch",
    "lint": "eslint .",
    "check": "flow check",
    "testonly": "mocha",
    "test": "flow check && mocha",
    "test.dev": "mocha --growl --watch",
    "analyze": "NODE_ENV=production webpack --config examples/app/webpack.config.babel.js --json | webpack-bundle-size-analyzer",
    "build.wp": "webpack --config examples/app/webpack.config.babel.js",
    "build.babel": "babel examples/app/src --out-dir dist/examples",
    "start": "webpack-dev-server --config examples/app/webpack.config.babel.js --hot --history-api-fallback --inline",
    "watch": "chokidar --initial -d 2000 -t 1000 'src/**/*.js' -c 'npm run build && cp -rvf *.js src dist $npm_package_config_dest/node_modules/reactive-di'"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/reactive-di.git"
  },
  "keywords": [
    "reactive-di",
    "dependency injection",
    "di",
    "modular",
    "state",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "flow-typed",
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-metadata": "^3.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "circular-dependency-plugin": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airplus": "^2.1.2",
    "flow-bin": "^0.47.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "inferno": "^3.3.1",
    "inferno-component": "^3.3.1",
    "inferno-dom": "^1.0.7",
    "jsdom": "^11.0.0",
    "jss": "^7.1.5",
    "jss-camel-case": "^4.0.0",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.3",
    "sinon": "^2.3.2",
    "babel-plugin-inferno": "^3.2.0",
    "source-map-loader": "^0.2.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "es6-error": "^4.0.2"
  }
}
